.warp {
  width: 100vw;
  padding: 0 30rpx 30rpx 30rpx;
  background: #fff;
}
.tbody {
  height: 500rpx;
  overflow-y: auto;
  overflow-x: hidden;
}
.w1 {
  width: 28%;
  padding-left: 0rpx !important;
  padding-right: 0rpx !important;
}
.w2 {
  width: 72%;
  text-align: left;
}
.wrap {
  width: 100%;
}
.demo-layout {
  box-shadow: 1px 1px 15px rgba(0, 0, 0, 0.3);
  height: 80rpx;
  width: 140rpx;
  padding: 20rpx;
  border-radius: 10rpx;
  font-size: 20rpx;
  margin: 15rpx 15rpx;
}
.parking {
  color: #fa3534;
  box-shadow: 1px 1px 15px rgba(250, 53, 52, 0.6);
}
.park-content {
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
  padding: 10rpx;
  margin-top: 30rpx;
}

