.div-table {
  display: table;
  width: 100%;
  height: 100%;
  border: 1rpx solid #ededed;
  box-sizing: border-box;
  table-layout: fixed;
  position: relative;
}
.div-table .celspan {
  display: table;
  width: 100%;
  height: 100%;
}
.div-table .celspan .td {
  display: table-cell;
  border: none !important;
}
.div-table .celspan .td + .td {
  border-left: 1rpx solid #ededed !important;
}
.div-table .rowspan {
  display: table;
  width: 100%;
  height: 100%;
}
.div-table .rowspan .tr {
  display: table-row;
  border: none !important;
}
.div-table.fixed-thead .tbody,
.div-table.fixed-thead .thead,
.div-table.fixed-thead .tr,
.div-table.fixed-thead .th,
.div-table.fixed-thead .td {
  display: block;
}
.div-table.fixed-thead .tr:after,
.div-table.fixed-thead .th:after {
  content: '';
  display: block;
  visibility: hidden;
  clear: both;
  height: 0;
}
.div-table.fixed-thead .td {
  float: left;
  color: #333;
  font-size: 26rpx;
}
.div-table .tr,
.div-table .th {
  display: table-row;
}
.div-table .tr + .tr .td,
.div-table .tr + .tr .th,
.div-table .tr + .th .td,
.div-table .tr + .th .th,
.div-table .th + .tr .td,
.div-table .th + .tr .th,
.div-table .th + .th .td,
.div-table .th + .th .th {
  border-top: 1rpx solid #ededed;
  word-break: break-word;
}
.div-table .rowspan .tr,
.div-table .rowspan .th {
  display: table-row;
}
.div-table .rowspan .tr .td,
.div-table .rowspan .tr .th,
.div-table .rowspan .th .td,
.div-table .rowspan .th .th {
