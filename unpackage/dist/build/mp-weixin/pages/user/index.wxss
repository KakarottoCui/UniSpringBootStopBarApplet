.warp_user{width:750rpx;padding:30rpx;background:linear-gradient(180deg,#3d7eff,#3d7eff 30%,#fff 90%)}.top_user_box{width:100%;display:flex;flex-direction:row;justify-content:flex-start;align-items:center;padding-bottom:30rpx}.top_user_box image{width:100rpx;height:100rpx;border-radius:100rpx;border:6rpx solid #fff}.top_user_box .user_info{padding:24rpx}.top_user_box .user_info .username{font-size:34rpx;color:#fff;padding-bottom:10rpx}.top_user_box .user_info .mobile{font-size:26rpx;color:#fff}.icon_boxs{background:#fff;border-radius:20rpx;padding:60rpx 0 20rpx 0;display:flex;flex-direction:row;justify-content:space-between;align-items:center}.icon_boxs .i{width:50%;text-align:center}.icon_boxs .i .icon_box{width:86rpx;height:86rpx;line-height:86rpx;border-radius:86rpx;background:#9ab6ff;display:flex;flex-direction:row;justify-content:center;align-items:center;margin:0 auto}.icon_boxs .i .n{padding-top:20rpx;font-size:24rpx;color:#333}.modal{position:fixed;top:0;left:0;width:100%;height:100%;background-color:rgba(0,0,0,.5);display:flex;justify-content:center;align-items:center;font-size:33rpx}.modal-content{background-color:#fff;width:600rpx;height:800rpx;border-radius:8rpx;position:relative}.modal-content view:first-child{padding:30rpx;font-size:33rpx;font-weight:700}.modal-content view:nth-child(2){padding:40rpx;font-size:33rpx;color:red}.modal-buttons{width:100%;display:flex;bottom:0;position:absolute;font-size:33rpx}.modal-buttons button{width:100%;border:none}.modal-buttons button:first-child{background-color:#74bfe7;color:#fff;border-radius:0}.modal-buttons button:last-child{width:100%;border:2rpx solid #74bfe7;border-radius:0;background-color:#fff;color:#74bfe7}